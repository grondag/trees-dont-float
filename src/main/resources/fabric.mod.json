{
  "schemaVersion": 1,
  "id": "tdnf",
  "version": "${version}",
  "name": "Trees Do Not Float",
  "description": "Trees - or anything made of logs - plus adjacent leaves fall down when no longer supported by a solid surface.",
  "authors": [
    "Grondag"
  ],
  "contact": {
    "homepage": "https://minecraft.curseforge.com/projects/tdnf",
    "sources": "https://github.com/grondag/tdnf"
  },
  "license": "Apache-2.0",
  "icon": "assets/tdnf/tdnf_icon.png",
  
  "environment": "*",
  "entrypoints": {
    "main": [
      "grondag.tdnf.TreesDoNotFloat"
    ]
  },
  
  "mixins": [
    "tdnf.server.json"
  ],
  
  "requires": {
    "fabricloader": ">=0.4.8",
    "fabric": ">=0.3.0"
  },
  "custom": {
    "modmenu:api": true,
    "modmenu:clientsideOnly": false
  }
}
