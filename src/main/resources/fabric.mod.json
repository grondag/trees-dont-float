  },
  "license": "Apache-2.0",
  "icon": "assets/tdnf/tdnf_icon.png",
  
  "environment": "*",
  "entrypoints": {
    "main": [ "grondag.tdnf.TreesDoNotFloat" ],
    "client": [ "grondag.tdnf.TreesDoNotFloat" ],
    "modmenu": [ "grondag.tdnf.ModMenuHelper" ]
  },
  
  "mixins": [
    "tdnf.server.json"
  ],
  
  "requires": {
    "fabricloader": ">=0.4.8",
    "fabric": ">=0.3.0"
  },
  "custom": {
