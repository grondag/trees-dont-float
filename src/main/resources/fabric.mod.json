{
  "schemaVersion": 1,
  "id": "trees-do-not-float",
  "version": "${version}",
  "name": "Trees Do Not Float",
  "description": "Trees - or anything made of logs - plus adjacent leaves fall down when the last supporting log is broken.",
  "authors": [
    "Grondag"
  ],
  "contact": {
    "homepage": "https://minecraft.curseforge.com/projects/trees-do-not-float",
    "issues": "https://github.com/grondag/trees-dont-float/issues"
  },
  "license": "Apache-2.0",
  "icon": "assets/tdnf/tdnf_icon.png",
  
  "environment": "*",
  "entrypoints": {
    "main": [ "grondag.tdnf.TreesDoNotFloat" ],
    "client": [ "grondag.tdnf.client.ClientInitializer" ],
    "modmenu": [ "grondag.tdnf.client.ModMenuHelper" ]
  },
  
  "mixins": [
    "tdnf.server.json"
  ],
  
  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": "1.17.x",
    "java": ">=16",
    "fabric": "*"
  },
  "custom": {
    "modmenu:api": false,
    "modmenu:clientsideOnly": false
  },
  "accessWidener": "tdnf.accesswidener"
}
